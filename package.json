{
  "name": "distill",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Automatic LLM agent migration from expensive to cheap models",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean && rm -rf node_modules .turbo",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "distill": "node --env-file=.env packages/cli/dist/index.js",
    "profile": "pnpm distill profile",
    "create-test-suite": "pnpm distill create-test-suite",
    "migrate": "pnpm distill migrate",
    "evaluate": "pnpm distill evaluate"
  },
  "devDependencies": {
    "turbo": "^2.5.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}
